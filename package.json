{
  "name": "microbetrace",
  "productName": "MicrobeTRACE",
  "description": "A Platform for Genomic Network Analysis",
  "version": "0.1.12",
  "author": "Tony Boyles <NSP3@cdc.gov>",
  "copyright": "Â©2018 CDC",
  "homepage": "http://aaboyles.github.io/MicrobeTrace",
  "license": "Apache-2.0",
  "repository": "http://github.com/CDCgov/MicrobeTrace",
  "main": "app/background.js",
  "build": {
    "appId": "com.cdc.microbetrace",
    "files": [
      "app/**/*",
      "node_modules/**/*",
      "package.json"
    ],
    "directories": {
      "buildResources": "resources"
    },
    "publish": null
  },
  "scripts": {
    "postinstall": "electron-builder install-app-deps",
    "preunit": "webpack --config=build/webpack.unit.config.js --env=test --display=none",
    "unit": "electron-mocha temp/specs.js --renderer --require source-map-support/register",
    "pree2e": "webpack --config=build/webpack.app.config.js --env=test --display=none && webpack --config=build/webpack.e2e.config.js --env=test --display=none",
    "e2e": "mocha temp/e2e.js --require source-map-support/register",
    "test": "npm run unit && npm run e2e",
    "start": "node build/start.js",
    "release": "npm test && webpack --config=build/webpack.app.config.js --env=production && electron-builder"
  },
  "dependencies": {
    "3d-force-graph": "^1.12.3",
    "alertifyjs": "^1.11.0",
    "bettermath": "^0.2.4",
    "bioseq": "^0.1.1",
    "bootstrap": "^3.3.7",
    "d3": "^4.10.2",
    "d3-force-3d": "^1.0.7",
    "d3-force-attract": "^0.1.1",
    "d3-sankey": "^0.7.1",
    "d3-symbol-extra": "^0.1.0",
    "datatables.net": "^1.10.16",
    "datatables.net-colreorder": "^1.4.1",
    "datatables.net-select": "^1.2.3",
    "fasta-parser": "0.1.0",
    "fs-jetpack": "^0.12.0",
    "is-online": "^7.0.0",
    "lazy.js": "^0.5.0",
    "marked": "^0.3.6",
    "neighbor-joining": "^1.0.4",
    "ntseq": "^1.1.0",
    "papaparse": "^4.3.6",
    "plain-hamming": "^0.1.0",
    "tn93": "^0.1.0",
    "underscore": "^1.8.3"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.5",
    "@babel/preset-env": "^7.0.0-beta.5",
    "babel-loader": "^8.0.0-beta.0",
    "babel-plugin-transform-object-rest-spread": "^7.0.0-beta.3",
    "chai": "^4.1.0",
    "css-loader": "^0.28.7",
    "electron": "^1.8.4",
    "electron-builder": "^19.43.3",
    "electron-mocha": "^5.0.0",
    "friendly-errors-webpack-plugin": "^1.6.1",
    "mocha": "^4.0.1",
    "source-map-support": "^0.5.0",
    "spectron": "^3.7.2",
    "style-loader": "^0.19.0",
    "webpack": "^3.8.1",
    "webpack-merge": "^4.1.0",
    "webpack-node-externals": "^1.6.0",
    "yarn": "^1.3.2"
  }
}
