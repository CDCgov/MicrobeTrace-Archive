{
  "name": "microbetrace",
  "productName": "MicrobeTrace",
  "description": "A Platform for Genomic Network Analysis",
  "version": "0.2.1",
  "author": "Tony Boyles <NSP3@cdc.gov>",
  "copyright": "Â©2017-2018 Centers for Disease Control and Prevention (CDC)",
  "homepage": "http://aaboyles.github.io/MicrobeTrace",
  "license": "Apache-2.0",
  "repository": "http://github.com/CDCgov/MicrobeTrace",
  "main": "app/background.js",
  "build": {
    "appId": "com.cdc.microbetrace",
    "files": [
      "app/**/*",
      "node_modules/**/*",
      "package.json"
    ],
    "directories": {
      "buildResources": "resources"
    },
    "publish": null,
    "win": {
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64",
            "ia32"
          ]
        },
        {
          "target": "portable",
          "arch": [
            "x64",
            "ia32"
          ]
        },
        {
          "target": "zip",
          "arch": [
            "x64",
            "ia32"
          ]
        }
      ],
      "publisherName": "Centers for Disease Control and Prevention (CDC)"
    }
  },
  "scripts": {
    "postinstall": "electron-builder install-app-deps",
    "preunit": "webpack --config=build/webpack.unit.config.js --env=test --display=none",
    "unit": "electron-mocha temp/specs.js --renderer --require source-map-support/register",
    "pree2e": "webpack --config=build/webpack.app.config.js --env=test --display=none && webpack --config=build/webpack.e2e.config.js --env=test --display=none",
    "e2e": "mocha temp/e2e.js --require source-map-support/register",
    "test": "npm run unit && npm run e2e",
    "start": "node build/start.js",
    "release": "npm test && webpack --config=build/webpack.app.config.js --env=production && electron-builder"
  },
  "dependencies": {
    "3d-force-graph": "^1.31.0",
    "@nteract/plotly": "^1.0.0",
    "alertifyjs": "^1.11.1",
    "bettermath": "^0.2.4",
    "bioseq": "^0.1.3",
    "bootstrap": "^3.3.7",
    "chosen-js": "^1.8.5",
    "d3": "^4.13.0",
    "d3-force-3d": "^1.1.0",
    "d3-force-attract": "^0.1.1",
    "d3-sankey": "^0.7.1",
    "d3-symbol-extra": "^0.1.0",
    "datatables.net": "^1.10.16",
    "datatables.net-bs": "^1.10.16",
    "datatables.net-colreorder": "^1.4.1",
    "datatables.net-scroller": "^1.4.4",
    "datatables.net-select": "^1.2.5",
    "datatables.net-select-bs": "^1.2.4",
    "fs-jetpack": "^0.12.0",
    "is-online": "^7.0.0",
    "lazy.js": "^0.5.1",
    "lodash": "^4.17.10",
    "msa": "^1.0.3",
    "neighbor-joining": "^1.0.4",
    "ntseq": "^1.1.0",
    "papaparse": "^4.4.0",
    "phylotree": "^0.1.6",
    "plain-hamming": "^0.1.0",
    "showdown": "^1.8.6",
    "shp-write": "^0.3.2",
    "tn93": "^0.1.1",
    "underscore.string": "^3.3.4",
    "xss": "^0.3.7"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.46",
    "@babel/preset-env": "^7.0.0-beta.46",
    "babel-loader": "^8.0.0-beta.2",
    "babel-plugin-transform-object-rest-spread": "^7.0.0-beta.3",
    "chai": "^4.1.0",
    "css-loader": "^0.28.11",
    "electron": "^1.8.5",
    "electron-builder": "^19.56.2",
    "electron-mocha": "^5.0.0",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "mocha": "^4.1.0",
    "source-map-support": "^0.5.5",
    "spectron": "^3.8.0",
    "style-loader": "^0.19.0",
    "webpack": "^3.11.0",
    "webpack-merge": "^4.1.2",
    "webpack-node-externals": "^1.7.2"
  }
}
